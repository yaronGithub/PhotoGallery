<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PhotoGallery.Practice"
             Title="Practice"
             x:Name="myLayout">
    <VerticalStackLayout>
        <Button
            x:Name="upBtn"
            Text="Read Only"
            Clicked="UpBtn_Clicked"/>
        <Button
            x:Name="lowBtn"
            Text="copy"
            Clicked="Copy"/>
        <Entry
            x:Name="myEntry"
            Placeholder="Enter a number"
            Keyboard="Numeric"
            />
        <Label
            x:Name="myLabel"
            Text=""
            />
    </VerticalStackLayout>
</ContentPage>